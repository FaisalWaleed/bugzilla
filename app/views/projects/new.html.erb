<div class="container">  
  <div class="row">
    <div class="col-sm-6 col-md-4 col-md-offset-4">
      <h1 class="text-center login-title">Start New Project</h1>
      <div class="account-wall">
<%= form_for @project do |new_project| %>
<div>
    <%= new_project.label :title %>
    <%= new_project.text_field :title, class: "form-control"  %><br>
</div>
<div class="field"><%= new_project.submit "Create", class: "btn btn-lg btn-success btn-block"  %></div>
<% end  %>
</div>
</div>
</div>
</div>